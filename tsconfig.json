{
  "compilerOptions": {"types": ["node", "ioredis", "jest"],
    "module": "commonjs",
    
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    
    "target": "ES2021",
    
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": false,
  
    "strictBindCallApply": false,
    "forceConsistentCasingInFileNames": false,
    "noFallthroughCasesInSwitch": false,   
  
    "noImplicitAny": true,
    //"sourceMap": true,
  //  "outDir": "./dist",
  //  "baseUrl": "./",
  //  "experimentalDecorators": true,
  //  "emitDecoratorMetadata": true,
    "strict": true,
    "strictPropertyInitialization": false,
  //  "skipLibCheck": true,
    "paths": {
      "*": ["node_modules/*"]
    },
   // "types": ["node", "jest"]
  },
  "include": ["src/**/*", "test/**/*"],
  "exclude": ["node_modules", "dist"]
}


